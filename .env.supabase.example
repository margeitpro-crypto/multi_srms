# Supabase Database Configuration
# Copy this file to .env.supabase and fill in your Supabase database details

# Supabase Database Connection Details
SUPABASE_DB_HOST=your-supabase-db-host.supabase.co
SUPABASE_DB_PORT=5432
SUPABASE_DB_NAME=your-database-name
SUPABASE_DB_USER=your-database-user
SUPABASE_DB_PASS=your-database-password

# API Configuration for Supabase
PORT=3002
VITE_API_URL=https://your-api-url.com/api

# JWT Secret for production environment
JWT_SECRET=your_strong_production_secret_key_here

# Frontend URL for production environment
FRONTEND_URL=https://yourdomain.com